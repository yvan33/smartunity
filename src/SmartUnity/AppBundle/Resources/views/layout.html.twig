<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}Welcome!{% endblock %}</title>
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

<!-- Additional meta content -->
    {% block meta %}{% endblock %}
    
<!-- Bootstrap core CSS -->
    <link rel="stylesheet" href="{{ asset('assets/css/bootstrap.css') }}">
    <link rel="stylesheet" href="{{ asset('assets/css/layout.css') }}">

<!-- Additional styleSheets -->
{% block stylesheets %}{% endblock %}


<!-- Bootstrap core JavaScript --> 
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
    <script>window.jQuery || document.write('<script src="{{ asset('js/jquery.js') }}"><\/script>')</script>
    <script src="{{ asset('assets/js/bootstrap.min.js') }}"></script>

<!-- Additional Javascript src -->
{% block initJS %}{% endblock %}


</head>
    
<!-- bg body -->
<body style="background-color:#334A64">
<!-- head -->
 <div class="container" style="background-color:#edebeb;box-shadow: 0 0 30px black;">
 
  <div class="row">
  <div class="col-md-12">
    <div class="row">
      <div class="col-md-offset-1 col-md-6"><img src="{{ asset('assets/img/logo-su.png') }}"  class="img-responsive logo cent-h"/></div>
      <div class="col-md-5 cent-h"><div class="cent-h logo2"><a href="#" class=" pos-re valign-col-all"><img src="{{ asset('assets/img/logo-iphon.png') }}"/><p class="red-dot">1</p></a><a href="#" class="valign-col-all pad-l"><p >trol_du_31</p></a></div></div>

      <ul class="nav navbar-nav ">
  <li><a href="{{ path('fos_user_registration_register') }}">Créer un compte</a></li>
  <li>{% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
Bonjour <a href="{{ path('fos_user_profile_show') }}">{{ app.user.username }}</a> <a href="{{ path('fos_user_security_logout') }}">Déconnexion</a>
{% else %}
<a href="{{ path('fos_user_security_login') }}">Connexion</a>

{% endif %}</li></ul>
    </div>
  </div>
  </div>
  
<!-- /.navbar-collapse -->
	<div class="row">
		<div class="col-md-12 menu-bg">
			<nav class="navbar navbar-inverse" role="navigation">
         <!--navbar- mobile -->
                  <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                      <span class="sr-only">Toggle navigation</span>
                      <span class="icon-bar"></span>
                      <span class="icon-bar"></span>
                      <span class="icon-bar"></span>
                  <a class="navbar-brand" href="#">Smart'Unity</a>
                    </button>
                  </div>
		<!--navbar- standar -->
                  <div class="collapse navbar-collapse navbar-ex1-collapse menu-mar" >
                    <ul class="nav navbar-nav">
                      <li class="home" ><a href="#"></a></li>
                      <li class="active"><a href="#"><p class="menu-pad">QUESTIONS & SOLUTIONS</p></a></li>
                      <li ><a href="#"><p class="menu-pad">BOUTIQUE</p></a></li>
                      <li ><a href="#"><p class="menu-pad">SMART'CAFE</p></a></li>
                    </ul>
                  </div>
         <!--navbar- standar fin --> 
			</nav>
		</div>
	</div>
<!-- end nav -->


<!-- BODY -->
<div class="row">
<div class="col-md-12" style="height:900px">

{% block body %}{% endblock %}

</div>
</div>

<!-- end BODY-->   

<!-- footer --> 
	<!-- footer separation --> 	
	<div class="row">
		<div class="col-md-12 foot-sep">
		</div>
	</div>
	<!-- footer separation fin --> 	
	<div class="row valign-row"  style="padding-top:20px; padding-bottom:20px; text-align:center">
		<div class="col-md-2 valign-col" ><img src="{{ asset('assets/img/sn.png') }}"/>
        	</div>
        <div class="col-md-3 valign-col col-mar"><a href="#"><img src="{{ asset('assets/img/logo-f-f.png') }}"  class="foot-im"/></a><a href="#"><img src="{{ asset('assets/img/logo-t-f.png') }}" class="foot-im"/></a><a href="#"><img src="{{ asset('assets/img/logo-g-f.png') }}" class="foot-im"/></a>
        	</div>
        <div class="col-md-2 valign-col"><a href="#"><p>A propos de smart'unity</p></a>
        	</div>
        <div class="col-md-2 valign-col col-mar"><a href="#"><p>Conditions generales d'utilisation</p></a>
        	</div>
        <div class="col-md-1 valign-col col-mar"><a href="#"><p>Publicité</p></a>
        	</div>
        <div class="col-md-2 valign-col col-mar"><a href="#"><p>Copy right smart'unity</p></a>
       		</div>
	</div>
<!-- end footer --> 

</div>

<!-- call Javascript & scripts -->
{% block callJS %}{% endblock %}



</body>
</html>

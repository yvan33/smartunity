{% extends "SmartUnityAppBundle::layoutLog.html.twig" %}

{% block title %}
    {{ 'mv_blog.blog_title'|parameter() }}
{% endblock %}
    
{% block body %}
    {% block blog_body %}{% endblock %} 
{% endblock %}    

{% block stylesheets %}
  {{ parent() }}
  <link rel="stylesheet" href="{{ asset('assets/css/blog/default.css') }}" type="text/css" media="all" />
  <link rel="stylesheet" href="{{ asset('assets/css/blog/blog.css') }}" type="text/css" media="all" />
  <link rel="stylesheet" href="{{ asset('assets/css/blog/colours.css') }}" type="text/css" media="all" />
{% endblock %}
{% block javascripts %}
  {{ parent() }}
{% endblock %}